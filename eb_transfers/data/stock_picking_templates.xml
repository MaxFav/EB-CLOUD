<odoo>
    <data>  
        <template id="report_eb_picking_operations">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.basic_layout">
                    <div class="page">
                        <div class="row">
                            <div class="col-6">
                              <span t-field="o.company_id.logo" t-options-widget="&quot;image&quot;" t-options-qweb_img_raw_data="1" style="width:149px;display:inline-block"/>
                            </div>
                            <div class="col-6 text-end">
                              <strong>Estella Bartlett Ltd</strong>
                              <span t-field="o.company_id.company_details"/>
                            </div>
                        </div>
                    </div>

                    <div class="row justify-content-end mt8 mb16">
                        <div class="col-6">
                            <div t-field="o.name" t-options="{'widget': 'barcode', 'width': 600, 'height': 100, 'img_style': 'width:300px;height:50px;'}"/>
                        </div>
                    </div>
                    
                </t>
            </t>
        </template> 
    </data>    
</odoo>