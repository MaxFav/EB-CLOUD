<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="estella_account_invoice_view_form_inherit" model="ir.ui.view">
        <field name="name">account.invoice.form.view</field>
        <field name="model">account.invoice</field>
        <field name="inherit_id" ref="account.invoice_form"/>
        <field name="priority" eval="1001"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='other_info']//field[@name='reference']" position="after">
                <field name='team_id'/>
            </xpath>
        </field>
    </record>
</odoo>